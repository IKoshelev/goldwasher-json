    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.6.47",
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "tslint": "^5.9.1",
    "typescript": "^3.1.1"
  }
}
